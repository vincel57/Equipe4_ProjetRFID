@model IEnumerable<ProjetRFID.Models.Simulation>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Analytique)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SVM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KNN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Random_Forest)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Analytique.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SVM.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KNN.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Random_Forest.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
